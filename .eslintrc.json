{
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "script",
    "ecmaFeatures": {
      "globalReturn": true
    }
  },
  "rules": {
    "for-direction": 2,
    "getter-return": 2,
    "no-async-promise-executor": 0,
    "no-const-assign": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 1,
    "no-dupe-else-if": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 1,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-import-assign": 2,
    "no-inner-declarations": 2,
    "no-irregular-whitespace": 2,
    "no-misleading-character-class": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-setter-return": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-labels": "warn",
    "no-unused-vars": 1,
    "no-useless-catch": 2,
    "no-useless-escape": 2,
    "no-with": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-constant-condition": 2,
    "no-cond-assign": 2
  },
  "env": {
    "node": true,
    "mocha": true,
    "commonjs": true,
    "es2020": true
  }
}
